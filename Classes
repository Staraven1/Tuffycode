class Cache :
    def __init__ (self,size):
        self.size = size


        
class Endpoint :
    def __init__ (self,Datacenterlatency):
        self.Datacenterlatency = Datacenterlatency
        self.CacheLatency = {}

    def ajouter_cache(self,Cache,latency):
        self.CacheLatency[Cache] = latency



